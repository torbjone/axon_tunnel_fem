proc celldef() {
  topol()
  subsets()
  geom()
  biophys()
  geom_nseg()
}

create axon[1]


proc topol() { local i
  basic_shape()
}
proc basic_shape() {
  axon[0] {pt3dclear()
  pt3dadd(-100, 0, 0, 0.5) pt3dadd(100, 0, 0, 0.5)}
}

objref all
proc subsets() { local i
  objref all
  all = new SectionList()
    axon all.append()

}
